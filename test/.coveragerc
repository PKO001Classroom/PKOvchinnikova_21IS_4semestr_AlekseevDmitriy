[run]
source = .
omit =
    tests/*
    venv/*
    .venv/*
    */__pycache__/*
    */site-packages/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*Test
    @pytest.fixture

[html]
directory = reports/coverage
title = Покрытие кода - Учебный журнал